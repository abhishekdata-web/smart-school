<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0,user-scalable=0" />
    <title>Gvn Ems</title>
    <link rel="manifest" href="/manifest.json" />
    <link rel="shortcut icon" href="/img/favicon.png" />
    <script>
        //check cookie w_auth
        let w_auth = document.cookie.indexOf('w_auth');
        if (w_auth != -1) {
            if (localStorage.getItem('role') != 10 && localStorage.getItem('role') != 2) {
                location.href = "/dashboard";
            }
        } else {
            location.href = "/login";
        }
    </script>
    <link rel="stylesheet" href="/css/main.min.css">

    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            background: #1F2946;
        }

        .user-container {
            padding: 20px;
        }

        .user-col {
            width: 23%;
        }

        @media only screen and (max-width: 600px) {
            .user-container {
                padding: 15px;
            }

            .user-col {
                width: 48%;
            }
        }

        .dropbtn {
            border-radius: 5px;
            margin-top: 5px;
        }

        input::placeholder {
            color: #c2c6dc;
        }

        table {
            border-collapse: collapse;
            width: 100%;
            color: #c2c6dc;
            font-size: 13px;
            white-space: nowrap;
            margin-bottom: 22px;
        }

        th {
            color: #fff;
            font-weight: 600;
            background: #262C49;
        }

        th,
        td {
            padding: 13px;
            border: 1px solid #414561;
        }

        tr:hover {
            background: #262C49;
        }

        .bottomscrollbar::-webkit-scrollbar {
            /* Hide scrollbar for Chrome, Safari and Opera */
            height: 9px;
            background: #262C49;
        }

        .bottomscrollbar::-webkit-scrollbar-thumb {
            background: #4D8AF0;
            border-radius: 25px;
        }

        /*print css*/
        .user-container {
            background: #121729;
        }

        .user-info-color {
            color: #fff;
        }

        .user-info-color span {
            color: #aaa;
        }

        .fee-ledger-title {
            color: #4D8AEF;
        }

        .print-particular {
            color: #4D8AEF;
        }

        @media print {
            .user-container {
                background: #fff;
                color: #000;
            }

            .user-info-color {
                color: #555;
            }

            .user-info-color span {
                color: #000;
            }

            table {
                color: #555;
            }

            th {
                color: #000;
                background: #fff;
            }

            th,
            td {
                border: 1px solid #4145618e;
            }

            .fee-ledger-title {
                color: #000;
            }

            .print-particular {
                color: #000;
            }
        }
    </style>
</head>

